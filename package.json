{
  "name": "version-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node release/index.js",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register 'src/**/*.spec.ts'",
    "local": "npm run clean && npm run build && development='true' node release/index.js",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build:start": "nodemon --exec npm start",
    "prebuild": "npm run clean && npm run copy:dotenv",
    "build": "tsc -p . --pretty",
    "build:watch": "npm run build -- -w",
    "clean": "rimraf release/*",
    "copy:dotenv": "cp-cli .env ./release/.env",
    "clean:dotenv": "rimraf ./release/.env",
    "rt": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/rt.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.6",
    "botkit": "^0.4.10",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.10",
    "redis": "^2.6.3",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "rxjs": "^5.4.3",
    "socket.io-client": "^2.0.3"
  },
  "devDependencies": {
    "@reactivex/rxjs": "^5.4.3",
    "@types/bluebird": "^3.5.2",
    "@types/lodash": "^4.14.65",
    "@types/mocha": "^2.2.43",
    "@types/moment-timezone": "^0.2.34",
    "@types/node": "^7.0.12",
    "@types/redis": "^0.12.36",
    "@types/request-promise-native": "^1.0.6",
    "@types/socket.io-client": "^1.4.30",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.6",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.1",
    "unit.js": "^2.0.0",
    "cp-cli": "^1.1.0"
  }
}
